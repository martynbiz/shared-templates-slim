{
    "name": "Shared templates",
    "description": "Combining a PHP framework with a JS framework via templates.",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "classmap": [
            "app/models",
            "app/views",
            "app/controllers",
            "tests/app",
            "tests/app/models",
            "tests/app/views",
            "tests/app/controllers"
        ]
    },
    "require": {
        "slim/slim": "2.*",
        "illuminate/database": "*",
        "xamin/handlebars.php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    }
}